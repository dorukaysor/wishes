{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "manage/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "manage/settings.json",
      "use": "@vercel/static"
    },
    {
      "src": "components/*",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/*",
      "use": "@vercel/static"
    },
    {
      "src": "styles/*",
      "use": "@vercel/static"
    },
    {
      "src": "assets/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/manage",
      "dest": "/manage/index.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
